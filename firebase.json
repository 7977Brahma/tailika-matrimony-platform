{
  "firestore": {
    "database": "(default)",
    "location": "asia-south1",
    "rules": "firebase/rules/firestore.rules",
    "indexes": "firebase/rules/firestore.indexes.json"
  },
  "functions": [
    {
      "source": "firebase/functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "storage": {
    "rules": "firebase/rules/storage.rules"
  }
}